{
    "name": "jdbcSinkConnector",
    "connector.class": "io.confluent.connect.jdbc.JdbcSicleankConnector",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://schema-registry:8081",
    "value.converter.enhanced.avro.schema.support": "true",
    "use.latest.version": "true",
    "value.converter.schemas.enable": "true",
    "topics": "customers, cities, regions, cells, products, rate_plans, product_rates, rate_tiers",
    "connection.url": "jdbc:postgresql://dw:5432/dw",
    "connection.user": "dw",
    "connection.password": "dw",
    "insert.mode": "insert",
    "table.name.format": "${topic}",
    "auto.create": "false",
    "auto.evolve": "false",
    "pk.mode": "record_value",
    "pk.fields": "id",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "header.converter": "org.apache.kafka.connect.storage.SimpleHeaderConverter",
    "delete.enabled": false,
    "error.tolerance": "all",
    "errors.deadletterqueue.topic.name": "jdbc-sink-dlq",
    "errors.deadletterqueue.topic.replication.factor":"1",
    "replace.null.with.default": true,
    "trim.sensitive.log": false,
    "errors.log.enable": true,
    "errors.log.include.messages": true,
    "errors.deadletterqueue.context.headers.enable": true
}